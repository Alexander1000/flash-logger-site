{
  "name": "Alexander1000/flash-logger-site",
  "description": "logs.dankovtsev.pro",
  "authors": [
    {
      "name": "Alexander Dankovtsev",
      "email": "a.dankovtsev@mail.ru"
    }
  ],
  "config": {
    "secure-http": false
  },
  "repositories": [
    { "packagist": false },
    { "type": "composer", "url": "http://packages.beauty-recommend.ru" }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.1",
    "phing/phing": "^2.16",
    "twig/twig": "2.4.4",

    "symfony/dependency-injection": "v4.0.4",
    "symfony/yaml": "v4.0.4",
    "symfony/config": "v4.0.4",
    "symfony/filesystem": "v4.0.4",

    "psr/log": "~1.0",
    "psr/http-message": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.3"
  },
  "autoload": {
    "psr-4": {
      "": "src",
      "Beauty\\": "beauty-framework/src"
    }
  }
}
